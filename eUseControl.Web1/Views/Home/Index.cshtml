@model dynamic

@{
	Layout = "~/Views/Shared/_Layout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <title>Klinik - Clinic Website Template</title>
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="" name="keywords">
    <meta content="" name="description">

    <!-- Favicon -->
    <link href="img/favicon.ico" rel="icon">

    <!-- Google Web Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin="">
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:wght@400;500&amp;family=Roboto:wght@500;700;900&amp;display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Libraries Stylesheet -->
    <link href="~/Content/lib/animate/animate.min.css" rel="stylesheet">
    <link href="~/Content/lib/owlcarousel/assets/owl.carousel.min.css" rel="stylesheet">
    <link href="~/Content/lib/tempusdominus/css/tempusdominus-bootstrap-4.min.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/Content/css/style.css" rel="stylesheet">
</head>
<body>
<section class="wrapper">

<!-- Header -->
<div class="container-fluid header bg-primary p-0 mb-5">
	@Html.Partial("_Header")
</div>

<!-- About -->
<div class="container-xxl py-5">
	@Html.Partial("_About")
</div>

<!-- Service -->
<div class="container-xxl py-5">
	@Html.Partial("_Service")
</div>

<!-- Feature -->
<div class="container-fluid bg-primary overflow-hidden my-5 px-lg-0">
	@Html.Partial("_Feature")
</div>

<!-- Team -->
<div class="container-xxl py-5">
	@Html.Partial("_Team")
</div>

<!-- Appointment -->
<div class="container-xxl py-5">
	@Html.Partial("_Appointment")
</div>

<!-- Testimonial -->
<div class="container-xxl py-5">
	@Html.Partial("_Testimonial")
</div>
</section>

</body>
</html>